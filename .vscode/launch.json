{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/app.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "externalTerminal",
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/js/*.js"]
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}